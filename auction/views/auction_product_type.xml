<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="product_type_action" model="ir.actions.act_window">
        <field name="name">Product Type</field>
        <field name="res_model">auction.product.type</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="iestate_property_tags_view_tree" model="ir.ui.view">
     	<field name="name">auction.product.type.view.tree</field>
      	<field name="model">auction.product.type</field>
      	<field name="arch" type="xml">
      		<tree string="Property Tag" editable = "bottom">
      			<field name="name" />
        	</tree>
      </field>
    </record>

    <record id="estate_property_tags_view_form" model="ir.ui.view">
	    <field name="name">auction.product.type.view.form</field>
	    <field name="model">auction.product.type</field>
	    <field name="arch" type="xml">
	        <form string="Property Tags">
	          <sheet>
	          <group>
	          	<field name="name"/>
	          </group>
	          </sheet>
	        </form>
	    </field>
	</record>
</odoo>